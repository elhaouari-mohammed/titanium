{
    "root": true,
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        // Additional, per-project rules...
        "eol-last": 1,
        "no-var":0,
        "arrow-parens":0,
        "linebreak-style":0,
        "comma-dangle":0
    }
}
